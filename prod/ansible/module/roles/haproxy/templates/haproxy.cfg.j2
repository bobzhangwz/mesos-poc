frontend webserver
    bind *:12345
    default_backend appservs
backend appservs
    balance roundrobin
    option forwardfor
    option httpchk HEAD / HTTP/1.1\r\nHost:localhost
    server k8s1 {{zk_ip}}:8080 check
    server k8s2 {{km_ip}}:8080 check
listen stats 
    bind {{zk_ip}}:1936
    mode http
    log global
    
    stats enable
    stats uri /
    stats hide-version
    stats auth someuser:password
